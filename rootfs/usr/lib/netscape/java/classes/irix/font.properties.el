fontcharset.monospaced.2=sun.awt.motif.CharToByteX11Dingbats
fontcharset.monospaced.3=sun.awt.CharToByteSymbol


fontcharset.dialog.0=sun.io.CharToByte8859_1
fontcharset.dialog.1=sun.io.CharToByte8859_7
